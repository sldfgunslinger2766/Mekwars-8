<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.dataFiles.SHouseData"
        table="SHouseData"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Integer"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="houseType"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="houseType"
        />

        <property
            name="baysProvided"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="baysProvided"
        />

        <property
            name="componentProduction"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="componentProduction"
        />

        <property
            name="members"
            type="java.util.Hashtable"
            update="true"
            insert="true"
            access="property"
            column="members"
        />

        <property
            name="money"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="money"
        />

        <property
            name="unitsOnMarket"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="unitsOnMarket"
        />

        <property
            name="infComponents"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="infComponents"
        />

        <property
            name="showProductionCountNext"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="showProductionCountNext"
        />

        <property
            name="abbreviation"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="abbreviation"
        />

        <property
            name="baseGunner"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="baseGunner"
        />

        <property
            name="basePilot"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="basePilot"
        />

        <property
            name="conquerable"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="conquerable"
        />

        <property
            name="houseColor"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="houseColor"
        />

        <property
            name="inHouseAttacks"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="inHouseAttacks"
        />

        <property
            name="initialHouseRanking"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="initialHouseRanking"
        />

        <property
            name="logo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="logo"
        />

        <property
            name="houseFluFile"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="houseFluFile"
        />

        <property
            name="mayUseBM"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="mayUseBM"
        />

        <property
            name="motd"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="motd"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SHouseData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
