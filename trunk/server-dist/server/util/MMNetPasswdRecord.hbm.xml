<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class 
        name="server.util.MMNetPasswdRecord" 
        table="Passwords" 
        proxy="server.util.MMNetPasswdRecord">
        
        <id 
            name="id" 
            column="playerId">
        <generator class="native"/>
        </id>
        
        <property name="userId" column="name" type="java.lang.String" not-null="true"/>
        <property name="passwd" column="password" type="java.lang.String" not-null="true"/>
        <property name="access" column="userlevel" not-null="true"/>
        <property name="time" column="lastUsed" not-null="true"/>
        <property name="playerLogo" column="logo" not-null="true"/>
    </class>
    
</hibernate-mapping>