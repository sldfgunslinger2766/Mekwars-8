<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.dataFiles.SArmyData"
        table="ArmyData"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="int"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="rawForceSize"
            type="double"
            update="true"
            insert="true"
            access="property"
            column="rawForceSize"
        />

        <property
            name="BV"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="BV"
        />

        <property
            name="locked"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="locked"
        />

        <property
            name="lowerLimiter"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="lowerLimiter"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
        />

        <property
            name="upperLimiter"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="upperLimiter"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SArmyData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
