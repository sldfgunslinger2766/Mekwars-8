<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.dataFiles.PlayerData"
        table="PlayerData"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Integer"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="activeSince"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="activeSince"
        />

        <many-to-one
            name="myHouseData"
            class="server.campaign.dataFiles.SHouseData"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="HOUSE_ID"
        />

        <list
            name="armies"
            table="playerArmies"
            lazy="true"
            inverse="false"
            cascade="none"
        >

              <key
                  column="armiesID"
              >
              </key>

              <index
                  column="Index"
              />

              <composite-element
                  class="server.campaign.dataFiles.SUnitData"
              >
        <property
            name="BV"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="BV"
        />

        <property
            name="hasDiedInRecentFight"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="hasDiedInRecentFight"
        />

        <property
            name="originalBV"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="originalBV"
        />

        <property
            name="scrappableUntil"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="scrappableUntil"
        />

        <property
            name="modelname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="modelname"
        />

        <property
            name="posId"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="posId"
        />

        <property
            name="producer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="producer"
        />

        <property
            name="status"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="status"
        />

        <property
            name="type"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="unitFilename"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="unitFilename"
        />

        <property
            name="weightclass"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="weightclass"
        />

              </composite-element>

        </list>

        <property
            name="attackRestrictionUntil"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="attackRestrictionUntil"
        />

        <property
            name="busySlices"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="busySlices"
        />

        <property
            name="chickenCount"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="chickenCount"
        />

        <property
            name="experience"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="experience"
        />

        <property
            name="fluffText"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fluffText"
        />

        <map
            name="gameoptions"
            table="playerGameOptions"
            lazy="true"
            sort="unsorted"
            inverse="false"
            cascade="none"
        >

              <key
                  column="gameOptionsID"
              >
              </key>

              <index
                  column="Index"
                  type="java.lang.String"
              />

              <element
                  column="Element"
                  type="java.lang.String"
                  not-null="false"
                  unique="false"
              />

        </map>

        <property
            name="hasScrappedThisTick"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="hasScrappedThisTick"
        />

        <property
            name="idle"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="idle"
        />

        <property
            name="immunetill"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="immunetill"
        />

        <property
            name="influence"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="influence"
        />

        <property
            name="lastCheckAttack"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="lastCheckAttack"
        />

        <property
            name="lastOnline"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="lastOnline"
        />

        <property
            name="lastScan"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="lastScan"
        />

        <property
            name="lastSentStatus"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="lastSentStatus"
        />

        <property
            name="leaderVote"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leaderVote"
        />

        <property
            name="money"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="money"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
        />

        <property
            name="onlineSince"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="onlineSince"
        />

        <property
            name="pricePenalty"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="pricePenalty"
        />

        <property
            name="rank"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="rank"
        />

        <property
            name="rating"
            type="double"
            update="true"
            insert="true"
            access="property"
            column="rating"
        />

        <property
            name="groupAllowance"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="groupAllowance"
        />

        <list
            name="units"
            table="playerUnts"
            lazy="true"
            inverse="false"
            cascade="none"
        >

              <key
                  column="unitsID"
              >
              </key>

              <index
                  column="Index"
              />

              <composite-element
                  class="server.campaign.dataFiles.SUnitData"
              >
        <property
            name="BV"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="BV"
        />

        <property
            name="hasDiedInRecentFight"
            type="boolean"
            update="true"
            insert="true"
            access="property"
            column="hasDiedInRecentFight"
        />

        <property
            name="originalBV"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="originalBV"
        />

        <property
            name="scrappableUntil"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="scrappableUntil"
        />

        <property
            name="modelname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="modelname"
        />

        <property
            name="posId"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="posId"
        />

        <property
            name="producer"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="producer"
        />

        <property
            name="status"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="status"
        />

        <property
            name="type"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="unitFilename"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="unitFilename"
        />

        <property
            name="weightclass"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="weightclass"
        />

              </composite-element>

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PlayerData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
