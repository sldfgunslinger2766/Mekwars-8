<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.util.PlayerDefectionEntry"
        table="PlayerDefections"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="ID"
            column="ID"
            type="java.lang.Integer"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="originHouseID"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="originHouseID"
        />

        <property
            name="playerID"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="playerID"
        />

        <property
            name="targetHouseID"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="targetHouseID"
        />

        <property
            name="timestamp"
            type="long"
            update="true"
            insert="true"
            access="property"
            column="timestamp"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PlayerDefectionEntry.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
