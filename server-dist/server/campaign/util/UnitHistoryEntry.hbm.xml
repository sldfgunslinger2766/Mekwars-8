<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.util.UnitHistoryEntry"
        table="UnitHistory"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="ID"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="action"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="action"
        />

        <property
            name="date"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="date"
        />

        <property
            name="source"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="source"
        />

        <property
            name="sourceHouse"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="sourceHouse"
        />

        <property
            name="target"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="target"
        />

        <property
            name="targetHouse"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="targetHouse"
        />

        <property
            name="unitID"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="unitID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UnitHistoryEntry.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
