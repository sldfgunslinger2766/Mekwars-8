<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.tasks.util.ComponentRaidTaskLog"
        table="ComponentRaidTaskLog"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="ID"
            column="ID"
            type="java.lang.Integer"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="amountStolen"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="amountStolen"
        />

        <property
            name="sizeStolen"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="sizeStolen"
        />

        <property
            name="typeStolen"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="typeStolen"
        />

        <property
            name="taskID"
            type="int"
            update="true"
            insert="true"
            access="property"
            column="taskID"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ComponentRaidTaskLog.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
