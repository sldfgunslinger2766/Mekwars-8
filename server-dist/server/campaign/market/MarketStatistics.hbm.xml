<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="server.campaign.market.MarketStatistics"
        table="MarketStatistics"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="ID"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="auctions"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="auctions"
        />

        <property
            name="gunnery"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="gunnery"
        />

        <property
            name="mechFileName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mechFileName"
        />

        <property
            name="piloting"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="piloting"
        />

        <property
            name="sum"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="sum"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-MarketStatistics.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
